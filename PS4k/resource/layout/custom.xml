<!-- synthetica.xml -->
<synth>
    <style id="button">
        <state>
            <insets top="6" left="6" bottom="6" right="6"/>
            <property key="Button.margin" type="insets" value="0 10 0 10"/>

            <defaultsProperty key="Synthetica.button.border.insets" type="insets" value="6 6 6 6"/>
            <defaultsProperty key="Synthetica.button.12x12.border.insets" type="insets" value="3 3 3 3"/>
        </state>
    </style>
    <bind style="button" type="region" key="Button"/>
</synth>
